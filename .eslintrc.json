{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true,
        "mongo": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": "warn" 
    }
}